    {
        "question": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><b>Sattriya, one of the classical dances of India, was introduced by which saint?</b></span></span></span></span></p>",
        "option_1": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Sankardev</span></span></span></span></span></p>",
        "option_2": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Tulsidas</span></span></span></span></span></p>",
        "option_3": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Kabir</span></span></span></span></span></p>",
        "option_4": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Chaitanya Mahaprabhu</span></span></span></span></span></p>",
        "option_5": "",
