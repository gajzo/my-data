        "option_1": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Gujarat</span></span></span></span></span></p>",
        "option_2": "<p style=\"orphans:2;widows:2\"> <span style=\"font-size:11pt\"><span><span style=\"font-style:normal\"><span style=\"text-decoration:none\"><span style=\"font-weight:normal\">Punjab</span></span></span></span></span></p>",
